.container
    width 100%
    max-width 100%
    margin 0 auto
    
    @media $bigDesktop
        width 1200px
        
.row
    display flex
    flex-wrap wrap
    min-width 100%
    
.col
    display flex
    padding 0 15px

col(n)
    width (100/(12/n))%
    
col_offset(n)
    margin-left (100/(12/n))%